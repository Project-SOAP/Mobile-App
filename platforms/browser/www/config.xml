<?xml version='1.0' encoding='utf-8'?>
<widget id="com.blanusse.SOAP" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>SOAP</name>
    <description>
        SOAP Application for drivers and technicians
    </description>
    <author email="baptiste.lanusse@ynov.com" href="http://www.ynov.com">
        Baptiste Lanusse
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
